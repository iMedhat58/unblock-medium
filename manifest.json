{
    "name": "Unblock Medium",
    "version": "1.0.0",
    "description": "Bypass Medium Website Blocking in some countries",
    "manifest_version": 3,
    "author": "Medhat ",
    "icons": {  "16": "icon16.png",
                "48": "icon48.png",
                "128": "icon128.png" },
    "permissions": [
        "tabs"
    ],
    "action":{
        "default_popup": "index.html",
        "default_title": "Unblock Medium"
    }
}